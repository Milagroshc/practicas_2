{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": [
        "microframework",
        "rest",
        "router",
        "psr7"
    ],
    "homepage": "http://github.com/slimphp/Slim-Skeleton",
    "license": "MIT",
    "authors": [{
        "name": "Josh Lockhart",
        "email": "info@joshlockhart.com",
        "homepage": "http://www.joshlockhart.com/"
    }],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "slim/twig-view": "^2.3",
        "illuminate/database": "~5.5.40",
        "slim/flash": "^0.1.0",
        "slim/csrf": "^0.5.0",
        "davedevelopment/phpmig": "^1.2",
        "zeuxisoo/slim-whoops": "^0.4.0",
        "carlosocarvalho/simple-input": "^1.0",
        "symfony/process": "^3.0",
        "hybridauth/hybridauth": "^2.3.0",
        "phpmailer/phpmailer": "6.0.*",
        "braintree/braintree_php": "3.34.0",
        "akrabat/ip-address-middleware": "^1.0",
        "mpdf/mpdf": "^8.0",
        "phpoffice/phpspreadsheet": "^1.8",
        "balping/json-raw-encoder": "^1.0",
        "culqi/culqi-php": "1.3.4",
        "rmccue/requests": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "paypal/rest-api-sdk-php": "dev-2.0-beta"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "App\\Controlador\\": "src/controlador",
            "App\\Modelo\\": "src/modelo",
            "App\\Mail\\": "src/Mail"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "start": "php -S miaplicativo.com.gob/aplicativo/public/ -t public public/index.php",
        "test": "phpunit"
    }
}
